---
pagination:
  data: collections.tags
  size: 1
  alias: tag
permalink: "{{ tag.name | tagNameToUrl }}index.html"
eleventyComputed:
  title: "Tag: {{ tag.name }}"
layout: 'layout.njk'
---
<ul class="posts">
  {%- for post in tag.posts -%}
    <li><a href="{{ post.url }}">{{ post.data.title }}</a> {% timeTagForDate post.date, 'short' %}</li>
  {%- endfor -%}
</ul>
